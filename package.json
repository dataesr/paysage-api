{
  "name": "paysage-api",
  "version": "0.0.1",
  "description": "API Paysage",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build:docs": "swagger-cli bundle src/openapi/openapi.yml --outfile docs/reference/openapi.yml --type yaml",
    "start": "npm run build:docs && NODE_ENV=development nodemon --experimental-specifier-resolution=node index.js",
    "start:production": "npm run build:docs && NODE_ENV=production node --experimental-specifier-resolution=node index.js",
    "start:staging": "npm run build:docs && NODE_ENV=staging node --experimental-specifier-resolution=node index.js",
    "test": "npm run build:docs && NODE_ENV=testing node --experimental-specifier-resolution=node --experimental-vm-modules node_modules/.bin/jest --watchAll --runInBand"
  },
  "keywords": [
    "paysage"
  ],
  "author": "DOAD",
  "license": "MIT",
  "dependencies": {
    "@cloudnative/health-connect": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-openapi-validator": "^4.12.11",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.6",
    "multer": "^1.4.4",
    "nodemailer": "^6.6.2",
    "swagger-ui-express": "^4.1.6",
    "swift": "^2.7.3",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.4",
    "swagger-cli": "^4.0.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./tests/globalSetup.js"
    ],
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "save"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
