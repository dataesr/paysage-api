{
  "extends": ["airbnb-base"],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": ["jest"],
  "env": {
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "max-classes-per-file": [
      "error",
      5
    ],
    "max-len": [
      "error",
      {
        "code": 150
      }
    ],
    "no-underscore-dangle": 0,
    "object-curly-newline": [
      "error",
      {
        "ObjectPattern": {
          "multiline": true,
          "minProperties": 6
        }
      }
    ]
  }
}
