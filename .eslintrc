{
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "plugins": [
    "jest"
  ],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "max-len": ["error", {"code": 120}],
    "no-underscore-dangle": 0,
    "object-curly-newline": [
      "error", {
        "ObjectPattern": {
          "multiline": true,
          "minProperties": 6
        }
      }
    ]
  }
}